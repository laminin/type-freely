{
  "name": "A browser action allows you to type in desired font",
  "description": "Changes the font of every input field.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Change Input Font",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "manifest_version": 2
}
